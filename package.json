{
  "name": "backendnode-expressapi",
  "version": "1.0.0",
  "description": "This is the backend using Node and Express for the store developed in react previously in the JavaScript school",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "lint": "eslint",
    "migration:generate": "sequelize-cli migration:generate --name",
    "migration:run": "sequelize-cli db:migrate",
    "migration:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "keywords": [
    "javascript",
    "backend",
    "node"
  ],
  "author": "Jose Corredor <jose.corrzaDeveloper@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "faker": "^5.5.3",
    "joi": "^17.6.0",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "6.6.5",
    "sequelize-cli": "^6.2.0"
  },
  "engines": {
    "node": "14.x"
  }
}
